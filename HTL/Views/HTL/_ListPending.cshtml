@model HashNetFramework.cHTL
<div class="portlet box green col-md-9">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-comments"></i>ALASAN PENDING PENGAJUAN
        </div>
    </div>
    <div class="portlet-body portlet-bodyies">
        @if ((ViewBag.user == "notaris" || ViewBag.user == "admin") && ViewBag.oprvalue != "view")
        {
        <div class="row">
            <div class="form-group" style="padding:0px 20px 0px 20px">
                <div class="col-md-12">
                    <label class="control-label">Pilih daftar berikut :</label>
                    @Html.DropDownListFor(m => m.CaseCaPendingbMulti, (IEnumerable<SelectListItem>)ViewData["SelectCasePending"], new { @class = "bs-select multi-select mlt", multiple = "multiple", @title = "Pilih Perihal ISUE " })
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="note note-warning">
                    <p>
                    </p>
                    Jika Perihal Pending tidak ada di "daftar list Pending" , silahkan untuk menghubungi team SIMASHT untuk ditambahkan
                    <p></p>
                </div>
            </div>
        </div>
        }
        else
        {
        <div class="row">
            <div class="form-group" style="padding:0px 20px 0px 20px">
                <div class="col-md-12">
                    <span class="form-control NonEdittableSpanTxt" disabled="disabled">@Model.CaseCabPendingDesc</span>
                </div>
            </div>
        </div>

        }
    </div>
</div>