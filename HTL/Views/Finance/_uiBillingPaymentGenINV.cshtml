@model HashNetFramework.cFilterContract
<div class="modal fade" id="filterdatadialogpaymentbilluplodINV" role="basic" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            @using (Ajax.BeginForm("clnOpenAddUplodINVFlag", new { Controller = "Finance" }, new AjaxOptions
            {
                HttpMethod = "POST"
            }, new
            {
                id = "billpayment_form_flag",
                @class = "form-horizontal",
                enctype = "multipart/form-data"
            }))
            {

            @Html.AntiForgeryToken()
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">Uoload Data Pembayaran</h4>
            </div>

            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="form-group" style="padding-left:20px">
                            <div class="col-md-6">
                                <label class="control-label">Jenis Pembayaran</label>
                                @Html.DropDownListFor(m => m.JenisTransaksi, (IEnumerable<SelectListItem>)ViewData["SelectTrans"], "Pilih..", new { @class = "select2 form-control cmb", @title = "Pilih " })
                            </div>
                        </div>
                    </div>
                    <div class="row" style="padding-left:20px">
                        <div class="col-md-6">
                            <label class="control-label">Upload File</label>
                            <input type="file" accept=".txt" name="upflebyr" id="upflebyr" />
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn green" data-dismiss="modal">
                    <i class="fa fa-close"></i>
                    <span>&nbsp;Tutup</span>
                </button>
                <button type="button" value="" class="btn green submtflag">
                    <i class="fa fa-check-square"></i>
                    <span>&nbsp;Proses </span>
                </button>
            </div>
            }
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

<script>
    $('.select2').select2();
    $('.date-picker').datepicker({
        rtl: App.isRTL(),
        orientation: "right bottom",
        autoclose: true
    });
    $.getScript("Scripts/vmFinance.js", function () {
    });
</script>