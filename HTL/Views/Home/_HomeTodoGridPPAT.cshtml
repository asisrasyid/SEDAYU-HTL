@model DusColl.vmHome
<style>
    td:not(:first-child) {
        white-space: nowrap;
    }


    td:last-child {
        width: 2%;
    }

    .btn {
        padding: 6px 7px;
        margin-bottom: 0;
        /*border-radius: 0;*/
        /*border: 0;*/
    }

    .btngridx {
        padding: 1px 5px;
        margin-bottom: 0;
    }

    .btngrid {
        padding: 6.7px 7px;
        margin-bottom: 0;
        border-radius: 0;
        border: 0;
    }
</style>


<table class="table table-striped table-hover table-bordered" id="table_txppat">
    <thead>
        <tr>

            <th>Nama <br /> PPAT</th>
            <th>Wilayah <br/> PPAT</th>
            <th>Pengecekan <br /> Sertifikat</th>
            <th>Sending<br />BPN</th>
            <th>Siap<br /> Akad</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.TodoNOT != null)
        {
            foreach (System.Data.DataRow item in Model.TodoNOT.Rows)
            {
                <tr class="DrillDownRow">
                    <td width="35%" align="left">@item["OrderKeNotaris"].ToString()</td>
                    <td width="35%" align="left">@item["Wilayah"].ToString()</td>
                    <td width="10%" align="center">@item["Pengecekan Sertifikat"].ToString()</td>
                    <td width="10%" align="center">@item["Sending BPN"].ToString()</td>
                    <td width="10%" align="center">@item["Siap Akad"].ToString()</td>
                </tr>
            }

        }

    </tbody>
</table>
