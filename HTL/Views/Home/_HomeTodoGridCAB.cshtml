@model DusColl.vmHome
<style>
    td:not(:first-child) {
        white-space: nowrap;
    }


    td:last-child {
        width: 2%;
    }

    .btn {
        padding: 6px 7px;
        margin-bottom: 0;
        /*border-radius: 0;*/
        /*border: 0;*/
    }

    .btngridx {
        padding: 1px 5px;
        margin-bottom: 0;
    }

    .btngrid {
        padding: 6.7px 7px;
        margin-bottom: 0;
        border-radius: 0;
        border: 0;
    }
</style>


<table class="table table-striped table-hover table-bordered" id="table_txcab">
    <thead>
        <tr>

            <th>Cabang</th>
            <th>Perbaikan</th>
            <th>Pending</th>
            <th>SiapAkad</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.TodoCAB != null)
        {
            var i = 0;
            foreach (System.Data.DataRow item in Model.TodoCAB.Rows)
            {
                <tr class="DrillDownRow">
                    <td width="30%" align="left">@item["NamaCabang"].ToString()</td>
                    <td width="10%" align="center">@item["Perbaikan"].ToString()</td>
                    <td width="10%" align="center">@item["Pending"].ToString()</td>
                    <td width="10%" align="center">@item["Siap Akad"].ToString()</td>
                </tr>
            }

        }

    </tbody>
</table>
