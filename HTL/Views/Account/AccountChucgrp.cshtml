@model DusColl.vmAccount
@{
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<div class="logo">
    <a>
        <h3 class="form-title font-green"> <img src="~/Content/assets/layouts/layout2/img/logo-fdcm-default-login-Blue.png" alt="logo" class="logo-default home" style="width:140px; padding-top: 0px;" /></h3>
    </a>
</div>
<!-- END LOGO -->
<!-- BEGIN LOGIN -->
<div class="content">
    <div class="col-md-12 page-404">
        <div class="row"></div>
        <div class="details">
            <h4>@ViewBag.caption</h4>
            <p style="color:#fff">
                @using (Html.BeginForm("clnAccountChucgongPro", "Account", FormMethod.Post, new { id = "frmaccountchangegrup", @class = "form-horizontal" }))
                {
                @Html.AntiForgeryToken();
                <div class="">
                    <div class="form-group">
                        <div class="col-md-12">
                            @Html.DropDownListFor(m => m.SelectGrupAkses, (IEnumerable<SelectListItem>)ViewData["SelectGrupAkses"], "Pilih..", new { @class = "form-control select2" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-12">
                            <button type="submit" class="btn green">
                                <i class="fa fa-check-square"></i><span>&nbsp;Ganti Grup Akses</span>
                            </button>
                        </div>
                    </div>
                </div>
                }
            </p>
            <p style="color:#fff">
                Klik Tombol ini
                <a href="@Url.Action("HomeGate","Home")">
                    <i class="icon-key"></i>Keluar
                </a>
                Untuk kembali ke halaman menu utama
            </p>
        </div>
    </div>
</div>

<script src="~/Content/assets/global/plugins/jquery.min.js" type="text/javascript"></script>
<script src="~/Content/assets/global/plugins/jquery-migrate.min.js"></script>
<script src="~/Content/assets/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<script src="~/Content/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
<script src="~/Content/assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
<script src="~/Content/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>

<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

<script src="~/Content/assets/global/scripts/app.js" type="text/javascript"></script>
<script src="~/Scripts/sweetalert.min.js"></script>
<script src="~/Scripts/vmAccount.js"></script>