@model DusColl.vmAccount
<style>
    #sample_editable_acctountmatrik td:not(:last-child) {
        white-space: nowrap;
    }

    #sample_editable_acctountmatrik td:last-child {
        width: 28%;
    }
</style>
<table class="table table-striped table-hover table-bordered" id="sample_editable_acctountmatrik">
    <thead>
        <tr>
            <th>Module</th>
            <th>View</th>
            <th>Add</th>
            <th>Edit</th>
            <th>Delete</th>
            <th>Approve</th>
            <th>Download</th>
            <th>Upload</th>
            <th>Generate</th>
            <th>Print</th>
            <th>Submit</th>
            <th>Cancel</th>
            <th></th>
        </tr>
    </thead>
    <tbody>


        @{
            if (Model.AccountMetrikList != null)
            {
                if (Model.AccountMetrikList.Count > 0)
                {
                    foreach (var item in Model.AccountMetrikList)
                    {
            <tr>
               
                <td>
                    @Html.Raw(item.MenuItem.ModuleName)
                </td>
                <td>
                    @Html.Raw(item.AllowView)
                </td>
                <td>
                    @Html.Raw(item.AllowAdd)
                </td>
                <td>
                    @Html.Raw(item.AllowEdit)
                </td>
                <td>
                    @Html.Raw(item.AllowDelete)
                </td>
                <td>
                    @Html.Raw(item.AllowApprove)
                </td>
                <td>
                    @Html.Raw(item.AllowDownload)
                </td>
                <td>
                    @Html.Raw(item.AllowUpload)
                </td>
                <td>
                    @Html.Raw(item.AllowGenerate)
                </td>
                <td>
                    @Html.Raw(item.AllowPrint)
                </td>
                <td>
                    @Html.Raw(item.AllowSubmit)
                </td>
                <td>
                    @Html.Raw(item.AllowCancel)
                </td>
                <td>
                     <a class="" href="javascript:;"  onclick="vmAccount.addmetrikgroup('@item.SecGroupName','@item.SecModuleID','upt')">
                        <i class="fa fa-edit"></i>
                        Edit </a><span>&nbsp;</span>
                    <a class="" href="javascript:;" onclick="vmAccount.deletemetrikgroup('@item.SecGroupName','@item.SecModuleID')">
                        <i class="fa fa-eraser"></i>
                        Delete</a>
                </td>

            </tr>
                    }
                }
            }}

    </tbody>
</table>

